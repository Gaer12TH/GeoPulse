# GeoPulse Environment Variables
# Copy this file to .env and fill in your actual values

# Google Apps Script Backend URL
VITE_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Contact IDs for chat system
VITE_CONTACT_1_ID=your_contact_1_id
VITE_CONTACT_2_ID=your_contact_2_id
VITE_CONTACT_3_ID=your_contact_3_id
VITE_CONTACT_4_ID=your_contact_4_id

# User ID for private mode
VITE_USER_ID=your_user_id

# Family IDs for notify mode
VITE_FAMILY_ID_1=your_family_member_1_id
VITE_FAMILY_ID_2=your_family_member_2_id
VITE_FAMILY_ID_3=your_family_member_3_id
